{
    "name": "inverted-framework/web",
    "description": "Web Components for the Inverted Framework for PHP",
    "keywords": ["PHP", "Inverted", "Web"],
    "homepage": "http://inverted-framework.github.io/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Doss"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "inverted-framework/core": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpdocumentor/phpdocumentor":"2.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.*",
        "phploc/phploc": "2.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/inverted-framework/core"
        }
    ]
}
